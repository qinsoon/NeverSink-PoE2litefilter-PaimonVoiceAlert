#===============================================================================================================
# [[0100]] Gold
#===============================================================================================================
# !! Waypoint c0.start : "Start - Override ALL rules"

Show # %D6 $type->gold $tier->stack3
	StackSize >= 5000
	BaseType == "Gold"
	SetFontSize 40
	SetTextColor 255 255 255 255
	SetBorderColor 255 255 255 255
	SetBackgroundColor 20 20 0 255
	PlayEffect Orange
	MinimapIcon 1 Yellow Cross

Show # %D6 $type->gold $tier->stack2
	StackSize >= 2500
	BaseType == "Gold"
	SetTextColor 255 255 255 255
	SetBorderColor 255 255 255 255
	PlayEffect Orange Temp
	MinimapIcon 1 White Cross

Show # %D5 $type->gold $tier->stack1
	StackSize >= 500
	BaseType == "Gold"
	SetTextColor 255 255 255 255
	SetBorderColor 255 255 255 255
	PlayEffect Orange Temp

Show # %D5 $type->gold $tier->stack1lvl
	StackSize >= 250
	BaseType == "Gold"
	AreaLevel < 65
	SetTextColor 255 255 255 255
	SetBorderColor 255 255 255 255
	PlayEffect Orange Temp

Hide # %H3 $type->gold $tier->any
	BaseType == "Gold"
	SetTextColor 180 180 180
	SetBorderColor 0 0 0 255
	SetBackgroundColor 20 20 0 180

